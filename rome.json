{
	"$schema": "https://docs.rome.tools/schemas/12.1.3/schema.json",
	"organizeImports": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true,
			"nursery": {
				"all": true,
				"useNamingConvention": "off",
				"noVoid": "off"
			},
			"suspicious": {
				"noConsoleLog": "off"
			}
		}
	},
	"javascript": {
		"formatter": {
			"semicolons": "asNeeded"
		}
	}
}